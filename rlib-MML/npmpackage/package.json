{
  "name": "@thinkridge/rlib-mml",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Music Macro Language Compiler (rlib-MML)",
  "keywords": [
    "mml",
    "music macro language",
    "wasm",
    "audio"
  ],
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build:wasm": "docker run --rm --name emsdk -v ./../rlib-MML/:/opt/vol -w /opt/vol -it emsdk \"cd wasm && mkdir -p build && cd build && emcmake cmake .. && make\"",
    "build": "npm run build:wasm && tsup src/index.ts --dts --format cjs,esm"
  },
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5.4.0",
    "tsup": "^8.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinkridge/rlib-Sounds",
    "directory": "rlib-MML/npmpackage"
  },
  "bugs": {
    "url": "https://github.com/thinkridge/rlib-Sounds/issues"
  },
  "homepage": "https://github.com/thinkridge/rlib-Sounds"
}